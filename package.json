{
  "name": "jojen",
  "version": "0.1.0-alpha.0",
  "description": "Expressive validator for Node and the browser, with a small footprint and awesome performance",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha -r test/_setup.js --compilers js:babel-register && eslint lib",
    "sizeBrowser": "browserify lib/index.js -t babelify --presets es2015 | ccjs --compilation_level=ADVANCED_OPTIMIZATIONS | gzip | wc -c",
    "prepublish": "babel lib --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WatchBeam/jojen.git"
  },
  "keywords": [
    "joi",
    "hapi",
    "validator",
    "jojen",
    "node",
    "browser"
  ],
  "author": "Connor Peet <connor@peet.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WatchBeam/jojen/issues"
  },
  "homepage": "https://github.com/WatchBeam/jojen#readme",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^5.0.0",
    "joi": "^8.0.0",
    "mocha": "^2.3.4"
  },
  "dependencies": {
    "deep-equal": "^1.0.1"
  }
}
